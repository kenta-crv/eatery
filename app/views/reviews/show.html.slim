- set_meta_tags  description: @review.description
br
br
br
#review-detail
  = link_to "一覧に戻る", reviews_path
  .fr-view
	  h1.headline-first  = @review.title
  br
  .fr-view
       - if @review.image_1?
        .image_review.center  = image_tag @review.image_1.url
  br
  table.company
     tbody
      tr
        th.arrow_box
           | 美味しさ
        td
           = @review.delicious_score
           | 点
        th.arrow_box
           | ムード
        td
           = @review.mood_score
           | 点
        th.arrow_box
           | CP
        td
           = @review.cost_performance_score
           | 点
        th.arrow_box
           | サービス
        td
           = @review.service_score
           | 点
      tr
        th.arrow_box
           | 創造性
        td
           = @review.imagination_score
           | 点
        th.arrow_box
           = @review.original1
        td
           = @review.original1_score
           | 点
        th.arrow_box
           = @review.original2
        td
           = @review.original2_score
           | 点
        th.arrow_box
           | 総合点
        td
           | 点
  table.company
   tbody
    .col[width="18%"]
    .col[width="18%"]
    .col[width="18%"]
    .col[width="18%"]
    .col[width="18%"]
    .col[width="18%"]
        tr
          th.arrow_box 訪問日
          td
            = @review.day
            th.arrow_box リピート価値
          td
            = @review.revisit
          th.arrow_box 平均単価
          td
            = @review.average_price
  br
  h1.headline-first.center  = @eatery.store
