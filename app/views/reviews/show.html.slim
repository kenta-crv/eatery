table.company
   colgroup
     col width="10%"
     col width="20%"
     col width="70%"
     tbody
      tr
        td
          strong.label 必須
        td.arrow_box
          | 店舗名
        td.center = @eatery.store
      tr
        td
          strong.label 必須
        td.arrow_box
          | テンポメイ
        td.center = @eatery.store_kana
      tr
        td
          strong.label 必須
        td.arrow_box
          | URL
        td.center = @eatery.url
      tr
        td
          strong.label 必須
        td.arrow_box
          | 電話番号
        td.center = @eatery.tel
      tr
        td
          strong.label 必須
        td.arrow_box
          | 住所
        td.center
          = @eatery.prefecture
          = @eatery.city
          = @eatery.town
          = @eatery.chome
          = @eatery.building
      tr
        td
          strong.label 必須
        td.arrow_box
          | ジャンル
        td.center = @eatery.genre
